<%- include('partials/header'); -%>
    <h2>
        <%= camp.name %>
    </h2>
    <img src="<%= camp.image %> ">
    <p>
        <%= camp.desc %>
    </p>
    <p>
        <a href="/camp/<%=camp._id%>/comments/new" class="btn btn-success">Add comment</a>
    </p>
    <br>
    <p>
        <%for(var comment of  camp.comments) {%>
            <p>
                <strong><%= comment.author %> </strong>
                <p>
                    <%= comment.title %>
                </p>
            </p>
            <% } %>
    </p>
    <%-include('partials/footer'); -%>